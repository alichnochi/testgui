local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/UI-Interface/CustomFIeld/main/RayField.lua'))()
local Window = Rayfield:CreateWindow({
   Name = "Arix Hub | Progressive",
   LoadingTitle = "Arix Interface Suite",
   LoadingSubtitle = "by Arrays",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Arrayfield"
   },
   Discord = {
      Enabled = false,
      Invite = "sirius", -- The Discord invite code, do not include discord.gg/
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Arrayfield",
      Subtitle = "Key System",
      Note = "Join the discord (discord.gg/sirius)",
      FileName = "SiriusKey",
      SaveKey = false,
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = "Hello"
   }
})
local New_Function_T = Window:CreateTab("New Function", 4483362458) -- Title, Image
local General_T = Window:CreateTab("General") -- Title, Image
local Section = New_Function_T:CreateSection("Section Example",true) -- The 2nd argument is to tell if its only a Title and doesnt contain element
-- Auto Farm Fox Lamp
local Fox_Lamp_C = New_Function_T:CreateToggle({
   Name = "Auto Farm Fox Lamp",
   Info = "...",
   CurrentValue = false,
   Callback = function(Value)
      Auto_Farm_Fox_Lamp = Value
   end
})
-- Auto Farm Shark Anchor
local Shark_Anchor_C = New_Function_T:CreateToggle({
   Name = "Auto Farm Shark Anchor",
   Info = "...",
   CurrentValue = false,
   Callback = function(Value)
      Auto_Farm_Shark_Anchor = Value
   end
})
-- Auto Fruit Sniper
local Auto_Fruit_Sniper_C = New_Function_T:CreateToggle({
   Name = "Auto Fruit Sniper",
   Info = "...",
   CurrentValue = false,
   Callback = function(Value)
      Auto_Fruit_Sniper = Value
   end
})
local Fox_C = New_Function_T:CreateToggle({
   Name = "Auto Bring Fruit",
   Info = "...",
   CurrentValue = false,
   Callback = function(Value)
      Auto_Farm_Fox_Lamp = Value
   end
})
